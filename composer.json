{
    "type": "library",
    "name": "jfnetwork/parapool",
    "license": "MIT",
    "description": "A library for parallel execution",
    "require": {
        "php": "7.4.*|8.0.*|8.1.*",
        "psr/log": "^1.0"
    },
    "autoload": {
        "psr-4": {
